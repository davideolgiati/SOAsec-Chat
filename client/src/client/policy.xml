<?xml version="1.0" encoding="UTF-8"?>
<wsp:Policy xmlns:sp="http://schemas.xmlsoap.org/ws/2005/07/securitypolicy" xmlns:wsp="http://schemas.xmlsoap.org/ws/2004/09/policy" >
	<sp:TransportBinding>
		<wsp:Policy>
			<sp:AlgorithmSuite>
				<wsp:Policy>
					<wsp:ExactlyOne>
						<sp:Basic256Rsa15 />
						<sp:TripleDesRsa15 />
					</wsp:ExactlyOne>
				</wsp:Policy>
			</sp:AlgorithmSuite>
			<sp:TransportToken>
				<wsp:Policy>
					<sp:HttpsToken RequireClientCertificate="false" />
				</wsp:Policy>
			</sp:TransportToken>
		</wsp:Policy>
	</sp:TransportBinding>

	<ramp:RampartConfig xmlns:ramp="http://ws.apache.org/rampart/policy">
		<ramp:passwordCallbackClass>PWCallbackService</ramp:passwordCallbackClass>
	</ramp:RampartConfig>

</wsp:Policy>